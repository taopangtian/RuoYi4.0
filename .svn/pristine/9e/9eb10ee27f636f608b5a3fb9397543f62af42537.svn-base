				<input type="button" id="" value="删除" class="s_delete border_radius s_buttom">
				<input type="button" id="" value="导入" class="s_import border_radius s_buttom">
				<input type="button" id="" value="导出" class="s_export border_radius s_buttom">
				<input type="button" id="" value="卸载网络" class="s_unin border_radius s_buttom">
				<div class="sech_right box_shadow border_radius">
					<ul>
						<li onclick="change()"><img src="/images/index_temli_0004_sousuo.png"  alt="搜索"/></li><li onclick="window.location.reload()"><img src="/images/index_temli_0005_shuaxin.png"  alt="搜索"/></li><li><img src="/images/index_temli_0006_gengduo.png"  alt="搜索"/></li>
