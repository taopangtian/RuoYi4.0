<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<title>终端管理</title>
		<link rel="stylesheet" type="text/css" href="static/owl/style.css">
		<script type="text/javascript">
		    // document.write('屏幕宽度：' + screen.width);
		    // document.write('屏幕高度：' + screen.height);
		</script>
	</head>
	<body>
		<!-- 头部导航开始 -->
		<!-- <embed type="text/html" src="nav.html"  width="100%" height="80px"/> -->
		<nav class="index_nav border_radius">
			<div class="index_logo">
				<img src="images/login_index_03.png" alt="logo">
			</div>
			<div class="index_header ">
				<ul>
					<li><div onclick="window.location.href='index.html'"  class="menu_home"></div></li>
					<li><div onclick="window.location.href='terminal.html'"  class="menu_audit div_selection"></div></li>
					<li><div onclick="window.location.href='index.html'" class="menu_setup"></div></li>
					<li><div onclick="window.location.href='index.html'" class="menu_statistics"></div></li>
					<li><div onclick="window.location.href='index.html'" class="menu_strategy"></div></li>
					<li><div onclick="window.location.href='personal.html'" class=" menu_terminal"></div></li>
				</ul>
			</div>
			<div class="index_ope">
				<ul>
					<li>
						<img src="images/login_index_18.png" alt="消息">
						<span class="s-top">10</span>
						<span>消息</span>
					</li>
					<li>
						<img src="images/login_index_20.png" alt="管理员">
						<span>管理员</span>
					</li>
					<li>
						<img src="images/login_index_22.png" alt="退出">
						<span>退出</span>
					</li>
				</ul>
			</div>
		</nav>
		<!-- 头部导航结束 -->
		
		<!-- 路径导航开始 -->
		<div class="route">
			<div class="route_magin_left route_x1 box_shadow border_radius">终端管理</div>
			<img src="images/login_index_01_00.png" alt="建号" class="route_magin_left">
		</div>
		<!-- 路径导航结束 -->
		
		<!-- 条件筛选开始 -->
		<section class="search box_shadow border_radius " id="section">
			<ul>
				<li>
					<span>责任人</span>
					<input type="text" name="search_zr" id="" placeholder="请输入责任人" required="" class="search_xuanz border_radius">
				</li>
				<li>
					<span>部门</span>
					<input type="text" name="search_bm" id="" placeholder="请输入部门名称" required="" class="search_xuanz border_radius">
				</li>
				<li>
					<span>IP</span>
					<input type="text" name="search_IP" id="" placeholder="请输入IP" required="" class="search_xuanz border_radius">
				</li>
				<li>
					<span>离线</span>
					<input type="text" name="search_lx" id="" placeholder="请输入选择离线时间" required="" class="search_xuanz border_radius">
					<div class="search_calendar">
						<img src="images/index_temli_0003_rili.png" alt="日历">
					</div>
				</li>
			</ul>
			<div class="buttom_max">
				<input type="button" id="" value="查询" class="s_query border_radius s_buttom">
				<input type="button" id="" value="清空" class="s_empty border_radius s_buttom">
			</div>
		</section>
		<!-- 条件筛选结束 -->
		<!-- 列表开始 -->
		<div class="tm_max border_radius box_shadow">
			<!-- 操作区开始 -->
			<div class="tm_oper">
				<input type="button" id="" value="增加" class="s_inc border_radius s_buttom">
				<input type="button" id="" value="修改" class="s_modify border_radius s_buttom">
				<input type="button" id="" value="删除" class="s_delete border_radius s_buttom">
				<input type="button" id="" value="导入" class="s_import border_radius s_buttom">
				<input type="button" id="" value="导出" class="s_export border_radius s_buttom">
				<input type="button" id="" value="卸载网络" class="s_unin border_radius s_buttom">
				<div class="sech_right box_shadow border_radius">
					<ul>
						<li onclick="change()"><img src="images/index_temli_0004_sousuo.png"  alt="搜索"/></li><li onclick="window.location.reload()"><img src="images/index_temli_0005_shuaxin.png"  alt="搜索"/></li><li><img src="images/index_temli_0006_gengduo.png"  alt="搜索"/></li>
					</ul>
				</div>
			</div>
			<!-- 操作区结束 -->
			
		</div>
		<!-- 列表结束 -->
		<script>
			function change(){
			 var divEle= document.getElementById('section');
			divEle.className='demo2';//改变样式
			
};
		</script>
	</body>
</html>
